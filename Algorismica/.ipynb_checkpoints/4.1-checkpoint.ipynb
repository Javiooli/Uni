   "source": [
    "def acronim(frase):\n",
    "    acronim = \"\"\n",
    "    fraseSplit = frase.split()\n",
    "    for i in range(len(fraseSplit)):\n",
    "        acronim += fraseSplit[i][0].upper()\n",
    "    return acronim"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 27,
   "metadata": {
    "id": "IJoMRg_5Zkj_"
   },
   "outputs": [],
   "source": [
    "assert acronim(\"Hola que tal\") == 'HQT'"
   ]
  }
